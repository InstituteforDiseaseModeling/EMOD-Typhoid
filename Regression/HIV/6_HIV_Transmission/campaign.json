{
    "Use_Defaults": 1,
    "Campaign_Name": "HIV Outbreak via Prevalence Increase", 
    "Events": [
        {
            "class": "CampaignEvent",
            "Event_Name": "Outbreak",
            "Start_Day": 1,

            ".class": "CampaignEventByYear",
            ".Start_Year": 1980,

            "Nodeset_Config": {
                "class": "NodeSetAll"
            },

            "Event_Coordinator_Config": {
                "Intervention_Config": {
                    "Event_Name": "Outbreak (not used)", 
                    "Antigen": 0, 
                    "Genome": 0, 
                    "Outbreak_Source": "PrevalenceIncrease", 
                    "Incubation_Period_Override": 1,
                    "class": "OutbreakIndividual"
                }, 
                "Target_Demographic": "Everyone", 
                "Demographic_Coverage": 0.5,
                "class": "StandardInterventionDistributionEventCoordinator",
                "Travel_Linked": 0 
            }
        },

        {
            "class": "CampaignEvent",
            "Event_Name": "Male circumcision for initial population", 
            "Start_Day": 1,

            "Nodeset_Config":
            {
                "class": "NodeSetAll"
            }, 

            "Event_Coordinator_Config": 
            {
                "class": "SimpleInterventionDistributionEventCoordinator",
                "Coverage": 0.5,

                "Intervention_Config":
                {
                    "class": "MaleCircumcision",
                    "Probability": 0.75
                }
            }
        },

        {
            "class": "CampaignEvent",
            "Event_Name": "HIVNeedsMaleCircumcision: Hand out MaleCircumcision",
            "Start_Day": 1,
            "Nodeset_Config":
            {
                "class": "NodeSetAll"
            },
            "Event_Coordinator_Config":
            {
                "class": "StandardInterventionDistributionEventCoordinator",
                "Demographic_Coverage": 1,
                "Intervention_Config":
                {
                    "class": "NodeLevelHealthTriggeredIV",
                    "Trigger_Condition": "HIVNeedsMaleCircumcision",
                    "Actual_IndividualIntervention_Config":
                    {
                        "class": "MaleCircumcision"
                    }
                }
            }
        },

        {
            "class": "CampaignEvent",
            "Event_Name": "Male circumcision at birth", 
            "Start_Day": 1e6,

            "Nodeset_Config":
            {
                "class": "NodeSetAll"
            }, 

            "Event_Coordinator_Config": 
            {
                "class": "StandardInterventionDistributionEventCoordinator",
                "Target_Demographic": "Everyone", 
                "Property_Restrictions": [],

                "Intervention_Config":
                {
                    "Demographic_Coverage": 1,
                    "class": "BirthTriggeredIV",

                    "Actual_IndividualIntervention_Config": 
                    {
                        "class": "HIVSigmoidByYearAndSexDiagnostic",
                        "Days_To_Diagnosis": 0,
                        "Abort_States": [],
                        "Cascade_State": "",

                        "Ramp_Min": 0.05,
                        "Ramp_Max": 0.60,
                        "Ramp_MidYear": 2000,
                        "Ramp_Rate": 1,
                        "Positive_Diagnosis_Event": "HIVNeedsMaleCircumcision"
                    }
                }
            }
